<html>

  <head>
    <title>
      Slack page
    </title>
    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/site.webmanifest' />
    <link
      rel='mask-icon'
      href='/fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    {{! <link href='css/normalize.css' rel='stylesheet' /> }}

    <link href='css/slack.css' rel='stylesheet' />

    <link
      rel='preconnect'
      href='https://fonts.googleapis.com'
    />
    <link
      rel='preconnect'
      href='https://fonts.gstatic.com'
      crossorigin
    />
    <link
      href='https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700&display=swap'
      rel='stylesheet'
    />
  </head>

  <body class='body'>
    <div class='page'>
      <section class='page_section'>
        <div class='page_content hero_section'>
          <div class='hero'>
            <span class='hero_label'>Slack Pictures</span>
            <h1 class='hero_title'>One platform for your
              team and your work</h1>
            <p class='hero_info'>All the features of Slack
              work together so you can, too.
            </p>
            <div class='hero_buttons'>
              <a href='#' class='button button_primary'>Try
                for free</a>
              <a href='#' class='button button_secondary'>
                See all features
              </a>
            </div>
          </div>

          <img src='/img/hero.png' alt='hero' class='hero_image' />

          <div class="brands">
            <img src="/img/brands/airbnb.png" alt="airbnb logo" class="brands_img">
            <img src="/img/brands/intuit.png" alt="intuit logo" class="brands_img">
            <img src="/img/brands/newscorp.png" alt="newscorp logo" class="brands_img">
            <img src="/img/brands/onemedical.png" alt="onemedical logo" class="brands_img">
            <img src="/img/brands/sonos.png" alt="sonos logo" class="brands_img">
            <img src="/img/brands/target.png" alt="tsrget logo" class="brands_img">
            <img src="/img/brands/uber.png" alt="uber logo" class="brands_img">
            <img src="/img/brands/us-department.png" alt="us department logo" class="brands_img">
          </div>
        </div>
      </section>
    </div>
  </body>
  {{> clear}}
</html>

